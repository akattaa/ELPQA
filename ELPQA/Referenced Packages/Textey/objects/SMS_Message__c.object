        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Resend Message</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

var c = confirm( &quot;Do you want to resend this message?&quot; )

if( c ) {
   var result = sforce.apex.execute( &quot;simplesms/SmsMessagesWebservice&quot;
                                   ,&quot;resendMessage&quot;
                                   ,{messageId:&quot;{!SMS_Message__c.Id}&quot;} );
   alert( result )
   location.reload();
}</url>
    </webLinks>
    <webLinks>
        <fullName>Resend_Messages</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Resend Messages</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

var c = confirm( &quot;Do you want to resend these messages?&quot; )

if( c ) {
   var ids = {!GETRECORDIDS( $ObjectType.SMS_Message__c )};
   var result = &apos;&apos;;

   if( ids.length ) {
      result = sforce.apex.execute( &quot;simplesms/SmsMessagesWebservice&quot;
                                   ,&quot;resendMessages&quot;
                                   ,{messageIds:ids} );
      alert( result )
      location.reload();
   } else {
      alert( &quot;You did not select any messages.&quot; );
   }
}</url>
    </webLinks>
