<apex:page standardController="simplesms__SMS_Message__c" recordsetvar="m" extensions="simplesms.Ctrl_ReplyFromMessage" 
           action="{!init}">
  <apex:form >
    <apex:pageBlock >
      <apex:pageMessages />
      <apex:pageBlockButtons location="top">
        <apex:commandButton value="Back" action="{!cancel}" />
      </apex:pageBlockButtons>
    </apex:pageBlock>
  </apex:form>
</apex:page>